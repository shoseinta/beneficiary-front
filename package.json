{
  "name": "beneficiary-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "embla-carousel-react": "^8.6.0",
    "jalaali-js": "^1.2.8",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "leaflet.locatecontrol": "^0.84.2",
    "lottie-web": "^5.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-multi-date-picker": "^4.5.2",
    "react-router-dom": "^7.6.0",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.29.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "analyze:html": "source-map-explorer \"build/static/js/*.js\" --html build/bundle-report.html",
    "analyze:json": "source-map-explorer \"build/static/js/*.js\" --json build/bundle-report.json",
    "size:js:gzip": "gzip-size build/static/js/*.js",
    "size:js:br": "brotli-size build/static/js/*.js",
    "size:css:gzip": "gzip-size build/static/css/*.css",
    "size:css:br": "brotli-size build/static/css/*.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "brotli-size-cli": "^1.0.0",
    "depcheck": "^1.4.7",
    "gzip-size-cli": "^5.1.0",
    "source-map-explorer": "^2.5.3"
  }
}
